{
  "name": "plantuml-bundle",
  "main": "./lib/plantuml-bundle",
  "version": "0.0.0",
  "description": "A bundle with multiple tools to handle PlantUML diagrams. Includes creating and displaying rendered uml diagrams and syntax highlighting.",
  "keywords": [
    "plantuml",
    "syntax",
    "highlighting",
    "diagram",
    "uml",
    "preview"
  ],
  "activationCommands": {
    "atom-workspace": [
      "plantuml-bundle:togglePreview",
      "plantuml-bundle:generatePNG",
      "plantuml-bundle:generateSVG",
      "plantuml-bundle:generateTXT"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatheusAze/plantuml-bundle.git"
  },
  "bugs": {
    "url": "https://github.com/MatheusAze/plantuml-bundle/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "fs-plus": "^2.0.0",
    "js-beautify": "^1.5.10",
    "node-plantuml": "^0.8.1"
  },
  "plantumlVersion": "1.2018.14",
  "files": [
    "vendor/plantuml.jar",
    "scripts/download.js"
  ],
  "scripts": {
    "postinstall": "node scripts/get-plantuml-jar.js"
  },
  "readmeFilename": "README.md"
}
